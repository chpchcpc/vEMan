# this loop will let the vEMain main window open as long as the user abort/exit it
F_MAINLOOP()
{
        RES="$AANS"
        echo "DEBUG: Res before: $RES"

        while [ "$RES" != "252" ]||[ "$RES" != "111" ];do

                F_ESXMGR
                RES="$AANS"
                echo "DEBUG: Res after: $RES"
        done

}

export -f F_MAINLOOP
